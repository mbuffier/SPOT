<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#efefef"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin">

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="60sp"
        android:layout_marginLeft="2sp"
        android:layout_marginRight="5sp"
        android:layout_marginTop="0sp"
        android:orientation="vertical" >


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >

        <ImageButton
            android:layout_marginBottom="10dp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/timeLine"
        android:scaleType="fitCenter"
        android:src="@drawable/maya"
        android:background="#efefef"
            android:onClick="goToHistorique"/>

        <ImageButton
            android:layout_marginBottom="10dp"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:id="@+id/amis"
            android:scaleType="fitCenter"
            android:src="@drawable/amis"
            android:background="#efefef"
            android:onClick="goToListAmis"/>


    </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <ImageButton
                android:id="@+id/art"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="2sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="0sp"
                android:layout_weight="1"
                android:padding="10dip"
                android:adjustViewBounds="true"
        		
        		android:scaleType="fitCenter"
				android:src="@drawable/art"
                android:background="@drawable/contour"
                android:contentDescription="@null"
                android:onClick="goToListMuseum"/>

            <ImageButton
                android:id="@+id/balade"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="2sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="0sp"
                android:layout_weight="1"
                android:padding="10dip"
                android:adjustViewBounds="true"
        		
        		android:scaleType="fitCenter"
                android:src="@drawable/balade"
                android:background="@drawable/contour"
                android:contentDescription="@null"
                android:onClick="goToListBalade"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <ImageButton
                android:id="@+id/resto"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="2sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="0sp"
                android:layout_weight="1"
                android:padding="10dip"
                android:adjustViewBounds="true"
        		
        		android:scaleType="fitCenter"
                android:src="@drawable/resto"
                android:background="@drawable/contour"
                android:contentDescription="@null"
                android:onClick="goToListResto"/>

            <ImageButton
                android:id="@+id/bar"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="2sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="0sp"
                android:layout_weight="1"
                android:padding="10dip"
                android:adjustViewBounds="true"
        		
        		android:scaleType="fitCenter"
                android:src="@drawable/bar"
                android:background="@drawable/contour"
                android:contentDescription="@null"
                android:onClick="goToListBar"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <ImageButton
                android:id="@+id/concert"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="2sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="0sp"
                android:layout_weight="1"
                
                android:adjustViewBounds="true"
        		android:padding="10dip"
        		android:scaleType="fitCenter"
                android:src="@drawable/concert"
                android:background="@drawable/contour"
                android:contentDescription="@null"
                android:onClick="goToListConcert"/>

            <ImageButton
                android:id="@+id/shopping"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5sp"
                android:layout_marginLeft="2sp"
                android:layout_marginRight="5sp"
                android:layout_marginTop="0sp"
                android:layout_weight="1"
                
                android:adjustViewBounds="true"
        		android:padding="10dip"
        		android:scaleType="fitCenter"
                android:src="@drawable/shopping"
                android:background="@drawable/contour"
                android:contentDescription="@null"
                android:onClick="goToListShopping"/>
        </LinearLayout>
    </LinearLayout>

    <Button
        android:id="@+id/boutonPlus"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/boutonrond2"
        android:gravity="center"
        android:text="@string/Plus"
        android:textColor="#efefef"
        android:textSize="35dp"
        android:onClick="goToAddEvent"/>

</RelativeLayout>